    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/real-estate-core-tests/bin/Debug/netcoreapp3.1/real-estate-core-tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}/real-estate-core-tests",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch WebAPI",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/real-estate-api/bin/Debug/net5.0/real-estate-api.exe",
            "args": [],
            "cwd": "${workspaceFolder}/real-estate-api",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}